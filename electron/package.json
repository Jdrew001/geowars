{
  "name": "phaser-3-typescript-electron-template",
  "version": "1.0.0",
  "main": "./main.js",
  "author": "Marco Chierchia",
  "license": "MIT",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/Jdrew001/geowars"
  },
  "scripts": {
    "copy": "yarn --cwd ../client build && node ./scripts/copy-from-client.js",
    "electron": "electron .",
    "dev": "yarn --cwd ../client dev --host & yarn electron",
    "preview": "yarn copy && yarn electron",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder",
    "build": "yarn copy && yarn app:dist",
    "build-app": "electron-builder build --win --publish never",
    "deploy-app": "electron-builder build --win --publish always"
  },
  "devDependencies": {
    "electron": "^21.4.2",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "cheerio": "^1.0.0-rc.12",
    "electron-updater": "^5.3.0",
    "fs-extra": "^10.1.0"
  },
  "build": {
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type"
    }
  },
  "nodeversion": "14.18.1",
  "npmversion": "8.13.2"
}
